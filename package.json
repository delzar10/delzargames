{
  "name": "delzar-games",
  "version": "1.0.0",
  "description": "Delzar Games",
  "engines": {
    "node": "4.4.7",
    "npm": "3.10.10"
  },
  "browserslist": [
    "> 1%",
    "ie > 9"
  ],
  "scripts": {
    "install-dependencies": "npm install",
    "comment-heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
    "comment-heroku-postbuild": "echo This runs afterwards.",
    "comment-postinstall": "npm install",
    "prestart": "./node_modules/.bin/babel-node build-scripts/start-message.js",
    "start": "NODE_ENV=development npm-run-all --parallel security-check start-server lint:watch start-mockapi",
    "prestart-offline": "./node_modules/.bin/babel-node build-scripts/start-message.js",
    "start-offline": "npm-run-all --parallel start-server lint:watch",
    "start-server": "./node_modules/.bin/babel-node build-scripts/server.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 8080",
    "share": "npm-run-all --parallel start-server localtunnel",
    "lint:watch": "npm run lint -- --watch",
    "lint": "esw webpack.config.*  src  build-scripts --color",
    "test": "mocha --reporter progress build-scripts/test-setup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "./node_modules/.bin/babel-node build-scripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 8081",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "NODE_ENV=production ./node_modules/.bin/babel-node build-scripts/build.js",
    "postbuild": "./node_modules/.bin/babel-node build-scripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "repository": {
    "author": "delzar10",
    "type": "git",
